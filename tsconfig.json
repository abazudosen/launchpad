{
  "compilerOptions": {
    "sourceMap": true,
    "lib": ["es7", "dom",  "esnext"],
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "target": "es6",
    "module": "es2015",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
  },
}
